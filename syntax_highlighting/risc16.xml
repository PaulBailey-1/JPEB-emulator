<NotepadPlus>
<UserLang name="RiSC-16 extended" ext="" udlVersion="2.1">
<Settings>
<Global caseIgnored="no" allowFoldOfComments="no" foldCompact="no" forcePureLC="0" decimalSeparator="0"/>
<Prefix Keywords1="no" Keywords2="no" Keywords3="yes" Keywords4="no" Keywords5="no" Keywords6="no" Keywords7="no" Keywords8="no"/>
</Settings>
<KeywordLists>
<Keywords name="Comments">00 01 02 03# 04((EOL))</Keywords>
<Keywords name="Numbers, prefix1"/>
<Keywords name="Numbers, prefix2"/>
<Keywords name="Numbers, extras1"/>
<Keywords name="Numbers, extras2"/>
<Keywords name="Numbers, suffix1"/>
<Keywords name="Numbers, suffix2"/>
<Keywords name="Numbers, range"/>
<Keywords name="Operators1"/>
<Keywords name="Operators2"/>
<Keywords name="Folders in code1, open"/>
<Keywords name="Folders in code1, middle"/>
<Keywords name="Folders in code1, close"/>
<Keywords name="Folders in code2, open"/>
<Keywords name="Folders in code2, middle"/>
<Keywords name="Folders in code2, close"/>
<Keywords name="Folders in comment, open"/>
<Keywords name="Folders in comment, middle"/>
<Keywords name="Folders in comment, close"/>
<Keywords name="Keywords1">add addi nand lui lw sw beq jalr tlbw rfe tocr fmcr sys nop mov lli cmp addc sub subc and or xor shl shr shlc shrc sshr rotl rotr not bz bp bn bc bo bnz bne beq jmp clf rfi bnc</Keywords>
<Keywords name="Keywords2">movi .space .fill push pop kpsh kpop</Keywords>
<Keywords name="Keywords3">0x 0b</Keywords>
<Keywords name="Keywords4"/>
<Keywords name="Keywords5">r0 r1 r2 r3 r4 r5 r6 r7 r0, r1, r2, r3, r4, r5, r6, r7,</Keywords>
<Keywords name="Keywords6">EXIT MODE_HALT WRITE_FRAME_BUFFER INT0 INT1 INT2 INT3 INT4 INT5 INT6 INT7 WRITE_TILE_DATA</Keywords>
<Keywords name="Keywords7"/>
<Keywords name="Keywords8"/>
<Keywords name="Delimiters"/>
</KeywordLists>
<Styles>
<WordsStyle name="DEFAULT" fgColor="FFFFFF" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0"/>
<WordsStyle name="COMMENTS" fgColor="C0C0C0" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0"/>
<WordsStyle name="LINE COMMENTS" fgColor="000000" bgColor="FFFFFF" fontStyle="2" nesting="0"/>
<WordsStyle name="NUMBERS" fgColor="FF80FF" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0"/>
<WordsStyle name="KEYWORDS1" fgColor="00FFFF" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0"/>
<WordsStyle name="KEYWORDS2" fgColor="00FF00" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0"/>
